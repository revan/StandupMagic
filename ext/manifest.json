{
  "name": "StandupMagic",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Generates a bulleted list of yesterday's events from Google Calendar",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "cookies",
    "*://*.asana.com/*"
  ]
}
